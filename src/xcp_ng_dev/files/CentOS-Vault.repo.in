[C@CENTOS_VERSION@-base]
name=CentOS-@CENTOS_VERSION@ - Base
baseurl=http://vault.centos.org/@CENTOS_VERSION@/os/$basearch/
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-7
exclude=ocaml*
enabled=1

